{
  "validation_rules": {
    "status_code": 200,
    "response_structure": {
      "new_facts": "array"
    }
  },
  "array_validations": {
    "new_facts": {
      "min_length": 1,
      "max_length": 1,
      "item_structure": {
        "data": {
          "entity_type": "overtime",
          "employee_number": "string",
          "hours": "number"
        }
      },
      "item_exact_matches": [
        {
          "data.hours": 5.0
        }
      ]
    }
  }
}